{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },

    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildType": "archive"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "path": "./android/app/build/outputs/bundle/production-release.aab",
        "package": "com.greenbasketkz.app"
      },
      "ios": {
        "path": "./ios/build/ios.ipa"
      }
    }
  }
}
